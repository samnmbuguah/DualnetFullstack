  },
  "homepage": "https://github.com/yourusername/yourrepository#readme",
  "devDependencies": {
    "mocha": "^10.4.0",
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
