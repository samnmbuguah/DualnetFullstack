{
  "name": "dualnet",
  "version": "1.0.0",
  "description": "My full-stack application",
  "scripts": {
    "build": "npm run build --prefix FrontendDualnet",
    "prestart": "npm run build && npm run createTables && npm run populateTables",
    "start": "node BackendDualnet/index.js",
    "predev-disabled": "npm start --prefix FrontendDualnet && npm run createTables && npm run populateTables",    
    "dev": "nodemon BackendDualnet/index.js",
    "startReact": "npm start --prefix FrontendDualnet",
    "testdb": "node BackendDualnet/config/Database.js",
    "createTables": "node BackendDualnet/config/Dbsync.js",
    "testSocket": "node BackendDualnet/services/TestSocket.js",
    "fetchPairs": "node BackendDualnet/services/GateioServices.js",
    "populateTables": "node -e 'const populateTables = require(\"./BackendDualnet/jobs/PopulateTables\"); populateTables();'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/yourrepository.git"
  },
